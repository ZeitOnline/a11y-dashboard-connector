{
  "apiVersion": "batch/v1beta1",
  "kind": "CronJob",
  "metadata": {
    "name": "a11y-dashbord-connector"
  },
  "spec": {
    "schedule": "*/10 * * * *",
    "jobTemplate": {
      "spec": {
        "template": {
          "spec": {
            "containers": [
              {
                "name": "a11y-dashbord-connector",
                "image": "registry.zeit.de/a11y-dashbord-connector:v0.1"
              }
            ],
            "restartPolicy": "OnFailure"
          }
        }
      }
    }
  }
}
